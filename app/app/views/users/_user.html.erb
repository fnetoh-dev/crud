<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Role:</strong>
    <%= user.role %>
  </p>

  <p>
    <strong>Active:</strong>
    <%= user.active %>
  </p>

  <p>
    <strong>Boolean:</strong>
    <%= user.boolean %>
  </p>

</div>
